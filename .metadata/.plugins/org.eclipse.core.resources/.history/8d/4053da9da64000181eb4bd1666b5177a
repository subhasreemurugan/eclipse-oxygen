package hashTable;

public class uniqchars {
	public static void main(String Args[]) {
		String s= "gfdsh mnb";
		int count =0;
		for (int i =0;i<s.length();i++) {
		  for(int j=0;j<s.length();j++) {
			  if(s.indexOf(i)==s.indexOf(j) && i!=j) {
				 break;  
			  }
			  count ++;
		  }
		}
		System.out.println(count);
	}
}
