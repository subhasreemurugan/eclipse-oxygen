package hashTable;

public class rev_str {
public static void main(String args[]) {
	String str= "ab cd ";
	char[] rev= reverse(str);
	System.out.println(rev);
}
public static char[] reverse( String str) {
	char [] rev_char=str.toCharArray();
	char[] rev=new char[rev_char.length];
	int j,i;
	for (i=0;i<rev_char.length; i++) {
		//System.out.println(j);
	  	   rev[i]=rev_char[rev_char.length-i-1];
	}
	return rev;
}
}
