package hashTable;
import java.util.*;


public class uniq_hash {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
      String s1 = "fdfj kjk";
      int k;
      boolean flag=true;
      Hashtable h= new Hashtable(); 
      for(int i=0;i<s1.length();i++) {
    	  k= (int) (s1.charAt(i));
    	  h.put(k, 1);  
      }
      for(int i=65;i<123;i++) {
    	  System.out.println(h.get(i));
    	  
      }
      	}

}
