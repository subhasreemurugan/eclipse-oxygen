package hashTable;

public class rev_str {
public static void main(String args[]) {
	String str= "abcd ";
	String rev= reverse(str);
	System.out.println(rev);
}
public static String reverse( String str) {
	String rev="";
	char [] rev_char=str.toCharArray();
	for (int i =rev_char.length;i>0;i++) {
	  	rev=rev+rev_char[i];
	}
	return rev;
}
}
