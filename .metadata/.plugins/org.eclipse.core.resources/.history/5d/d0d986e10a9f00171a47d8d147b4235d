class GeometricObject:
    def __init__(self):
        pass
class triangel(GeometricObject):
    def __init__(self, s1 = 1, s2 = 1, s3 = 1):
        super().__init__()
        self.s1=s1
        self.s2=s2
        self.s3=s3
        
    def area(self):
        s=(s1+s2+s3)/2
        a=(s*(s-s1)*(s-s2)(s-s3))**0.5
    
        
        