class GeometricObject:
    def __init__(self):
        pass
class triangel(GeometricObject):
    def __init__(self, s1 = 1, s2 = 1, s3 = 1):
        super().__init__()
        self.s1=s1
        self.s2=s2
        self.s3=s3
        
    def area(self):
        s=(self.s1+self.s2+self.s3)/2
        a=(s*(s-self.s1)*(s-self.s2)(s-self.s3))**0.5
        return a
        
    def peri(self):
        p=(self.s1+self.s2+self.s3)
        return p